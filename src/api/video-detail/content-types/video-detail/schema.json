{
  "kind": "collectionType",
  "collectionName": "video_details",
  "info": {
    "singularName": "video-detail",
    "pluralName": "video-details",
    "displayName": "video_detail",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "video_name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "text",
      "required": true,
      "unique": true
    },
    "master_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::master-category.master-category",
      "inversedBy": "video_details"
    },
    "master_tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::video-tag.video-tag",
      "inversedBy": "video_details"
    },
    "subcategories": {
      "type": "enumeration",
      "enum": [
        "Hybrid",
        "Indica",
        "Sativa"
      ]
    },
    "video": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "videos"
      ]
    },
    "V_name": {
      "type": "string",
      "customFieldConfig":{
        "fieldRenderer":"video-upload"
      }
    }}
}
